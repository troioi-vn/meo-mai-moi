{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bezhansalleh/filament-shield": "^3.3",
        "darkaonline/l5-swagger": "^9.0",
        "fakerphp/faker": "^1.24",
        "filament/filament": "^3.3",
        "intervention/image": "^3.11",
        "laravel/framework": "^12.0",
        "laravel/jetstream": "^5.3",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10.1",
        "spatie/laravel-medialibrary": "^11",
        "stechstudio/filament-impersonate": "^3.16",
        "symfony/http-client": "^7.3",
        "symfony/mailgun-mailer": "^7.3",
        "tomatophp/filament-users": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.82",
        "larastan/larastan": "^3.0",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "nunomaduro/phpinsights": "^2.13",
        "phpunit/phpunit": "^11.5",
        "deptrac/deptrac": "^2.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan migrate --graceful --ansi"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "format": [
            "./vendor/bin/php-cs-fixer fix"
        ],
        "phpstan": [
            "./vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "phpstan:baseline": [
            "./vendor/bin/phpstan analyse --generate-baseline --memory-limit=2G"
        ],
        "deptrac": [
            "./vendor/bin/deptrac analyse"
        ],
        "deptrac:baseline": [
            "./vendor/bin/deptrac analyse --report-uncovered --formatter=baseline"
        ],
        "insights": [
            "@php artisan insights --no-interaction --min-quality=80 --min-complexity=75 --min-architecture=75 --min-style=75"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

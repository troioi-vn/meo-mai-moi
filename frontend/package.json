{
  "name": "frontend",
  "private": true,
  "version": "0.6.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && bun run blade:update && bun run assets:copy",
    "build:analyze": "ANALYZE=true vite build",
    "build:docker": "vite build && bun run blade:update && bun run assets:copy",
    "assets:copy": "node ./scripts/copy-assets.cjs",
    "blade:update": "node ./scripts/update-blade.cjs",
    "lint": "eslint . --cache",
    "lint:deps": "depcruise src --config .dependency-cruiser.cjs",
    "lint:dead": "ts-prune --project tsconfig.app.json",
    "test": "vitest run",
    "test:silent": "vitest run --reporter=dot --silent",
    "test:minimal": "vitest run --reporter=dot --silent --no-coverage",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "test:backend": "cd ../backend && php artisan test --parallel",
    "e2e": "./scripts/e2e-test.sh",
    "e2e:direct": "playwright test",
    "e2e:ui": "SKIP_E2E_SETUP=true playwright test --ui",
    "e2e:report": "playwright show-report",
    "test:e2e": "./scripts/e2e-test.sh",
    "preview": "vite preview",
    "format": "prettier --write .",
    "api:generate": "orval",
    "api:watch": "orval --watch",
    "api:check": "orval && git diff --exit-code src/api/generated",
    "i18n:scan": "i18next-scanner --config i18next-scanner.config.cjs",
    "i18n:unused": "node scripts/find-unused-translations.cjs",
    "i18n:clean": "node scripts/remove-unused-translations.cjs"
  },
